{
  "name": "@ohos/faultloggerd",
  "description": "Collecting c/c++ runtime crash log",
  "version": "3.1",
  "license": "Apache License 2.0",
  "publishAs": "code-segment",
  "segment": {
      "destPath": "base/hiviewdfx/faultloggerd"
  },
  "component": {
    "name": "faultloggerd",
    "subsystem": "hiviewdfx",
    "syscap": [],
    "features": [],
    "adapted_system_type": [
      "small",
      "standard"
    ],
    "rom": "1024KB",
    "ram": "~1024KB",
    "deps": {
      "components": [
        "common",
        "init",
        "hilog_native",
        "c_utils",
        "hisysevent_native"
      ],
      "third_party": [
        "libunwind",
        "bounds_checking_function"
      ]
    },
    "build": {
      "sub_component": [
        "//base/hiviewdfx/faultloggerd/interfaces/innerkits/backtrace:libbacktrace_local",
        "//base/hiviewdfx/faultloggerd/interfaces/innerkits/signal_handler:dfx_signalhandler",
        "//base/hiviewdfx/faultloggerd/interfaces/innerkits/dump_catcher:libdfx_dumpcatcher",
        "//base/hiviewdfx/faultloggerd/interfaces/innerkits/faultloggerd_client:libfaultloggerd",
        "//base/hiviewdfx/faultloggerd/interfaces/rust:libpanic_handler",
        "//base/hiviewdfx/faultloggerd/interfaces/rust:libstacktrace_rust",
        "//base/hiviewdfx/faultloggerd/tools/crash_validator:crash_validator",
        "//base/hiviewdfx/faultloggerd/tools/dump_catcher:dumpcatcher",
        "//base/hiviewdfx/faultloggerd/tools/process_dump:processdump",
        "//base/hiviewdfx/faultloggerd/services:faultloggerd"
      ],
      "inner_kits": [
        {
          "name": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/backtrace:libbacktrace_local",
          "header": {
            "header_files": [
              "backtrace_local.h",
              "catchframe_local.h"
            ],
            "header_base": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/backtrace/include"
          }
        },
        {
          "name": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/dump_catcher:libdfx_dumpcatcher",
          "header": {
            "header_files": [
              "dfx_dump_catcher.h"
            ],
            "header_base": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/dump_catcher/include"
          }
        },
        {
          "name": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/faultloggerd_client:libfaultloggerd",
          "header": {
            "header_files": [
              "faultloggerd_client.h"
            ],
            "header_base": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/faultloggerd_client/include"
          }
        },
        {
          "name": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/signal_handler:dfx_signalhandler",
          "header": {
            "header_files": [
              "dfx_signal_handler.h"
            ],
            "header_base": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/signal_handler/include"
          }
        },
        {
          "name": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/callstack:callstack_perf",
          "header": {
            "header_files": [
              "callstack.h",
              "register.h"
            ],
            "header_base": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/callstack/include"
          }
        },
        {
          "name": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/callstack:callstack_nativedaemon",
          "header": {
            "header_files": [
              "callstack.h",
              "register.h"
            ],
            "header_base": "//base/hiviewdfx/faultloggerd/interfaces/innerkits/callstack/include"
          }
        }
      ],
      "test": [
        "//base/hiviewdfx/faultloggerd/example:dumpcatcherdemo",
        "//base/hiviewdfx/faultloggerd/test:faultloggerd_fuzzertest",
        "//base/hiviewdfx/faultloggerd/test:faultloggerd_moduletest",
        "//base/hiviewdfx/faultloggerd/test:faultloggerd_systemtest",
        "//base/hiviewdfx/faultloggerd/test:faultloggerd_performancetest",
        "//base/hiviewdfx/faultloggerd/test:faultloggerd_funchook",
        "//base/hiviewdfx/faultloggerd/test:faultloggerd_unittest",
        "//base/hiviewdfx/faultloggerd/tools/crasher_c:crasher_c",
        "//base/hiviewdfx/faultloggerd/tools/crasher_cpp:crasher_cpp",
        "//base/hiviewdfx/faultloggerd/tools/panic_maker:panic_maker"
      ]
    }
  }
}